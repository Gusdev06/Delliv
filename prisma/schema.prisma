generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int         @id @default(autoincrement())
  guid        String      @unique @default(uuid())
  name        String
  description String
  price       Float
  category    String
  createdAt   DateTime
  updatedAt   DateTime

}